  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nipunaw/ClariFi/issues"
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.11",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "react-scripts": "^4.0.3"
  },
  "build": {
    "appId": "com.electron.clarifi",
